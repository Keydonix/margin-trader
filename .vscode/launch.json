	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Test",
			"skipFiles": [ "<node_internals>/**" ],
			"cwd": "${workspaceFolder}/test/",
			"runtimeArgs": [ "-r", "ts-node/register", ],
			"args": [ "${workspaceFolder}/test/index.ts", ],
			"env": {
				"TS_NODE_PROJECT": "${workspaceFolder}/test/tsconfig.json",
			}
		}
	]
}
