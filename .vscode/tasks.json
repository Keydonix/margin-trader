{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "docker-compose",
			"type": "process",
			"command": "docker-compose",
			"args": [
				"up",
				"--force-recreate",
				"--always-recreate-deps",
				"--abort-on-container-exit",
				"--remove-orphans",
				"--renew-anon-volumes"
			],
			"problemMatcher": []
		}
	]
}
